  },
  "devDependencies": {
    "@handy-common-utils/dev-dependencies": "^1.0.19",
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/av": "^0.4.1",
    "@types/js-yaml": "^4.0.3",
    "@types/lodash": "^4.14.172",
    "@types/prompts": "^2.0.14"
  },
  "dependencies": {
    "@handy-common-utils/oclif-utils": "^1.0.4",
    "@handy-common-utils/promise-utils": "^1.1.1",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.3",
    "av": "^0.4.9",
    "fast-xml-parser": "^3.19.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "microsoft-cognitiveservices-speech-sdk": "^1.18.1",
    "mp3": "^0.1.0",
    "multi-integer-range": "^4.0.9",
    "murmurhash": "^2.0.0",
    "prompts": "^2.4.1",
    "speaker": "^0.5.3"
